{
  "family": "mytask-def",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::289202785684:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "nodecont",
      "image": "289202785684.dkr.ecr.ap-southeast-2.amazonaws.com/myecr-repo",
      "cpu": 0,
      "portMappings": [
        {
          "name": "myport",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mytask-def",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "volumes": []
}
